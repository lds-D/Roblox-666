local function RotateIconAnimation()
    while true do
        RotateIcon.Rotation = RotateIcon.Rotation + 1
        wait(0.01)
    end
end

coroutine.wrap(RotateIconAnimation)()

local function ToggleTab(tab)
    ScriptsFrame.Visible = tab == "Scripts"
    SettingsFrame.Visible = tab == "Settings"
    ScriptsTab.BackgroundColor3 = tab == "Scripts" and Color3.fromRGB(0, 170, 255) or Color3.fromRGB(40, 40, 60)
    SettingsTab.BackgroundColor3 = tab == "Settings" and Color3.fromRGB(0, 170, 255) or Color3.fromRGB(40, 40, 60)
end

ScriptsTab.MouseButton1Click:Connect(function()
    ToggleTab("Scripts")
end)

SettingsTab.MouseButton1Click:Connect(function()
    ToggleTab("Settings")
end)

CloseButton.MouseButton1Click:Connect(function()
    ScreenGui:Destroy()
end)

local KeyVerified = false

SubmitButton.MouseButton1Click:Connect(function()
    if KeyBox.Text == "lajcken943" then
        KeyVerified = true
        KeyStatus.Text = "Status: Verified"
        KeyStatus.TextColor3 = Color3.fromRGB(0, 255, 0)
        KeyFrame.Visible = false
        MainFrame.Visible = true
    else
        KeyStatus.Text = "Status: Invalid Key"
        KeyStatus.TextColor3 = Color3.fromRGB(255, 0, 0)
    end
end)

if not KeyVerified then
    MainFrame.Visible = false
    KeyFrame.Visible = true
end

